{
    "name": "mrgoodbytes8667/http-client-common",
    "type": "library",
    "description": "Some common files needed for the various HttpClient backed packages",
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/http-client": "^5.2",
        "symfony/http-foundation": "^5.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "symfony/http-foundation": "<5.2.1"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\HttpClient\\Common\\": "src/"
        }
    }
}
