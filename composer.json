{
    "name": "mrgoodbytes8667/http-client-common",
    "type": "library",
    "description": "Some common files needed for the various HttpClient backed packages",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/http-client": "^6.2",
        "symfony/http-foundation": "^6.2"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0.9",
        "symfony/var-dumper": "^6.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpunit/phpunit": "<9.5",
        "symfony/http-foundation": "<5.2.1"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\HttpClient\\Common\\": "src/",
            "Bytes\\HttpClient\\Common\\Tests\\": "Tests/"
        }
    }
}
